input(rel="search" type="search" ng-show="")
table(class="table table-hover")
    thead
        td #
        td Имя
        td Фамилия
        td Телефон
        td email
    tr(ng-repeat="item in items" ng-click="editItems(item.$id)")
        td {{item.index + 1}}
        td {{item.name.first}}
        td {{item.name.last}}
        td {{item.phone}}
        td {{item.email}}